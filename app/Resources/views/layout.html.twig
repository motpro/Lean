<!DOCTYPE html>
<html lang="en" ng-app>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
    <meta name="robots" content="noindex,nofollow" />
    <title>{% block title %}{% endblock %}</title>
    <script src="{{ asset('bundles/angularjs/angular.min.js') }}"></script>
    <script src="http://libs.baidu.com/jquery/1.9.0/jquery.js"></script>
    {% block head %}{% endblock %}
</head>
<body style="padding-top: 70px;">

{% block navbar %}{% endblock %}

<div id="content">
    <div class="header-logo">

    </div>

    <div class="container">
        <div class="row">
            {% block body %}{% endblock %}
        </div>
    </div>
</div>


<script type="text/javascript" src="{{ asset('bundles/bootstrap3/dist/js/bootstrap.min.js') }}"></script>
{% block javascript %}

{% endblock %}
<script type="text/javascript" charset="utf-8" src="{{ asset( 'bundles/jquery/jquery.contextmenu.r2.packed.js') }}"></script>


</script>
</body>
</html>
